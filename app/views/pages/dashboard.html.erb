<div class="w-full">
  <div class="flex flex-col items">
    <%= render 'pages/shared/tabs' %>
  </div>
  
  <%= turbo_frame_tag :dashboard, class:'w-full bg-green-200' do %>
    <h1 class="font-bold text-4xl">Pages#dashboard</h1>
    <p>Find me in app/views/pages/dashboard.html.erb</p>

    <% @customers.each do |customer| %>
      <div class="flex flex-col items-center">
        <h1 class="font-bold text-2xl"><%= customer.name %></h1>
      </div>
    <% end %>
  <% end %>
</div>